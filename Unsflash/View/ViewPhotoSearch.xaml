    <Grid Background="#bf000000">
        <Grid Name="Noreult" Visibility="Collapsed" VerticalAlignment="Center" HorizontalAlignment="Center">
            <Border Background="White"
                                        Grid.Column="3"
                                        CornerRadius="8"
                                        Margin="4,0,4,0"
                                        Opacity="0.8">
                <Button IsEnabled="True" Name="btNoreult">
